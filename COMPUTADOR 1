package sem2;

public class Setup {
		
	String cor;
	String modelo;
	String marca;
	String preco;
	      
	public String getCor() {
	    return cor;
	}
	public void setCor(String cor) {
	    this.cor = cor;
	}
	public String getModelo() {
	    return modelo;
	}
	public void setModelo(String modelo) {
	    this.modelo = modelo;
	}
	public String getMarca() {
	    return marca;    
	}
	public void setMarca(String marca) {
		this.marca = marca;
	}
	public String getPreco() {
	    return preco;    
	}
	public void setPreco(String preco) {
		this.preco = preco;
	}
	public String ligar() {
	    return "O pc está ligando";
	}
	public String desligar() {
	    return "O monior está desligando";
	}	
	public String mexer() {
    return "Você está mexendo";
    }
	public String pressionar() {
	    return "Você está digitando";
	}
}
